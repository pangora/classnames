{
    "name": "pangora/classnames",
    "description": "Elegant classlist builder with conditionals",
    "keywords": [
        "pangora",
        "classnames",
        "utility",
        "php"
    ],
    "homepage": "https://github.com/pangora/classnames",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jarand K. LÃ¸keland",
            "email": "jarand@pangora.no",
            "homepage": "https://www.pangora.no",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^3.16"
    },
    "autoload": {
        "psr-4": {
            "Pangora\\Classnames\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pangora\\Classnames\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "psalm": "vendor/bin/psalm -c psalm.xml --show-info=true"
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "issues": "https://github.com/pangora/classnames/issues",
        "source": "https://github.com/pangora/classnames",
        "docs": "https://docs.spatie.be/classnames"
    }
}
